[mysqld]
character_set_server = utf8
collation_server = utf8_unicode_ci
default_storage_engine = MyISAM
interactive_timeout = 300
join_buffer_size = 4M
key_buffer_size = 1G
max_allowed_packet = 512M
open_files_limit = 10000
query_cache_limit = 2M
query_cache_size = 16M
sql_mode = ''
wait_timeout = 300

[mysqldump]
max_allowed_packet = 512M
